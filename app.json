{
  "expo": {
    "name": "Fivedo",
    "slug": "fivedo",
    "description": "A prioritized five item todo list.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fivedo.jpeg",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.jamesmcarthur.fivedo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
